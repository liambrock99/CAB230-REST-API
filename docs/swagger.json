{
  "openapi": "3.0.0",
  "info": {
    "title": "CAB230: Web Computing API",
    "description": "Express API based on the CAB230 Web Computing API by Liam Brock.",
    "version": "1.0.0",
    "termsOfService": "http://swagger.io/terms/",
    "contact": { "email": "liam.brock99@gmail.com" },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "basePath": "/api",
  "tags": [
    {
      "name": "Authentication"
    },
    {
      "name": "Search"
    },
    {
      "name": "Helpers"
    }
  ],
  "paths": {
    "/register": {
      "post": {
        "tags": ["Authentication"],
        "description": "Register",
        "operationID": "post_register",
        "produces": "application/json",
        "responses": {
          "201": {
            "description": "Created"
          },
          "400": {
            "description": "Bad Request"
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": ["Authentication"],
        "description": "Login",
        "operationID": "post_login",
        "produces": ["application/json"],
        "requestBody": {
          "required": "true",
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "username": "string",
                  "password": "string"
                }
              },
              "example": {
                "username": "user@email.com",
                "password": "password123"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/search": {
      "get": {
        "tags": ["Search"],
        "description": "Login",
        "operationID": "get_search",
        "produces": "application/json",
        "parameters": [
          {
            "in": "query",
            "name": "offence",
            "description": "The offence to search for",
            "schema": {
              "type": "string"
            },
            "required": "true"
          },
          {
            "in": "query",
            "name": "area",
            "description": "List of comma seperated areas to filter search results by",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "age",
            "description": "List of comma seperated ages to filter search results by",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "gender",
            "description": "List of comma seperated genders to filter search results by",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "year",
            "description": "List of comma seperated years to filter search results by",
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "month",
            "description": "List of comma seperated genders to filter search results by",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "query": {
                      "type": "object",
                      "properties": {
                        "offence": {
                          "type": "string"
                        }
                      },
                      "example": {
                        "offence": "Homicide (Murder)"
                      }
                    },
                    "result": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "LGA": {
                            "type": "string"
                          },
                          "total": {
                            "type": "integer"
                          },
                          "lat": {
                            "type": "number"
                          },
                          "lng": {
                            "type": "number"
                          }
                        }
                      },
                      "example": [
                        {
                          "LGA": "Aurukun Shire Council",
                          "total": 4,
                          "lat": -27.470812,
                          "lng": 153.022455
                        }
                      ]
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/offences": {
      "get": {
        "tags": ["Helpers"],
        "description": "Full listing of offences",
        "operationId": "get_offences",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "offences": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": [
                        "Assault",
                        "Armed Robbery",
                        "Advertising Prostitution"
                      ],
                      "description": "List of offences to search by"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/areas": {
      "get": {
        "tags": ["Helpers"],
        "description": "Full listing of areas",
        "operationId": "get_areas",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "areas": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": [
                        "Brisbane City Council",
                        "Gold Coast City Council"
                      ],
                      "description": "List of areas to filter by"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/ages": {
      "get": {
        "tags": ["Helpers"],
        "description": "Full listing of ages",
        "operationId": "get_ages",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "ages": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": ["Adult", "Juvenile"],
                      "description": "List of ages to filter by"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/genders": {
      "get": {
        "tags": ["Helpers"],
        "description": "Full listing of genders",
        "operationId": "get_genders",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "genders": {
                      "type": "array",
                      "items": {
                        "type": "string"
                      },
                      "example": ["Male", "Female", "Not stated"],
                      "description": "List of genders to filter by"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/years/": {
      "get": {
        "tags": ["Helpers"],
        "description": "Full listing of years",
        "operationId": "get_years",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "years": {
                      "years": "array",
                      "items": {
                        "type": "integer"
                      },
                      "example": [2001, 2002, 2003, 2004],
                      "description": "List of years to filter by"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/months": {
      "get": {
        "tags": ["Helpers"],
        "description": "Full listing of months",
        "operationId": "get_months",
        "produces": ["application/json"],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "months": {
                      "type": "array",
                      "items": {
                        "type": "integer"
                      },
                      "example": [1, 2, 3, 4],
                      "description": "List of months to filter by"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
